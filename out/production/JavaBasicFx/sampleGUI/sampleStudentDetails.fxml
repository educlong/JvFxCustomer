<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ContextMenu?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="54.0" prefWidth="283.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sampleGUI.ControllerStudentDetails">
   <children>
      <HBox prefHeight="4.0" prefWidth="283.0">
         <children>
            <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="27.0" prefWidth="242.0" text="Student Details">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <Button fx:id="btnBackMainDemo" mnemonicParsing="false" text="Back">
               <contextMenu>
                  <ContextMenu>
                    <items>
                      <MenuItem mnemonicParsing="false" text="Unspecified Action" />
                    </items>
                  </ContextMenu>
               </contextMenu></Button>
         </children>
      </HBox>
      <HBox prefHeight="20.0" prefWidth="181.0">
         <children>
            <Label prefHeight="17.0" prefWidth="32.0" text="ID:" />
            <Label fx:id="lblIdStudentDetailDemo" prefHeight="17.0" prefWidth="45.0" text="Label" />
            <Label prefHeight="17.0" prefWidth="50.0" text="Name:">
               <HBox.margin>
                  <Insets left="10.0" />
               </HBox.margin>
            </Label>
            <Label fx:id="lblNameStudentDetailsDemo" prefHeight="17.0" prefWidth="207.0" text="Label" />
         </children>
      </HBox>
      <HBox prefHeight="18.0" prefWidth="181.0">
         <children>
            <Label prefHeight="17.0" prefWidth="39.0" text="Email:" />
            <Label fx:id="lblEmailStudentDetailDemo" prefHeight="17.0" prefWidth="185.0" text="Label" />
            <Label prefHeight="17.0" prefWidth="32.0" text="Age:">
               <HBox.margin>
                  <Insets left="10.0" />
               </HBox.margin>
            </Label>
            <Label fx:id="lblAgeStudentDetailDemo" prefHeight="17.0" prefWidth="34.0" text="Label" />
         </children>
      </HBox>
   </children>
</VBox>
